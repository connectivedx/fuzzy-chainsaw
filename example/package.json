{
  "name": "fuzzy-chainsaw",
  "version": "2.0.0",
  "description": "Boilerplate for component-based UI development",
  "repository": "connectivedx/fuzzy-chainsaw",
  "bugs": {
    "url": "https://github.com/connectivedx/fuzzy-chainsaw/issues"
  },
  "author": "Connective DX",
  "license": "MIT",
  "directories": {
    "source": "./source",
    "dest": "./dist"
  },
  "scripts": {
    "postinstall": "npm run dll",
    "start": "http-server ./dist -p 8081",
    "dev": "gulp clean:pre && npm-run-all --parallel dev:webpack dev:test",
    "dev:webpack": "webpack-dev-server --config ./build/webpack.dev-config.js --progress",
    "dev:test": "karma start --auto-watch",
    "dll": "gulp webpack-dll:build --progress",
    "dll:ci": "gulp webpack-dll:build:ci --progress",
    "dll:production": "gulp webpack-dll:production --progress",
    "dll:production:ci": "gulp webpack-dll:production:ci --progress",
    "build": "gulp build",
    "build:ci": "gulp build:ci",
    "production": "gulp production",
    "production:ci": "gulp production:ci",
    "full:build": "npm-run-all dll build",
    "full:build:ci": "npm-run-all dll:ci build:ci",
    "full:production": "npm-run-all dll:production production",
    "full:production:ci": "npm-run-all dll:production:ci production:ci",
    "test": "gulp test",
    "watch": "gulp watch",
    "new:tag": "gulp scaffold:tag --name",
    "new:component": "gulp scaffold:component --name",
    "new:composition": "gulp scaffold:composition --name",
    "new:stateful": "gulp scaffold:component:stateful --name"
  },
  "engines": {
    "node": "^=6.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.7.7",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "gulp": "^3.9.1",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-discard-empty": "^2.1.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.0.3",
    "postcss-remove-root": "^0.0.2",
    "postcss-reporter": "^4.0.0",
    "prismjs": "^1.6.0",
    "querystring": "^0.2.0",
    "reset-css": "^2.2.0",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "tape": "^4.7.0",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "dom-select": "^1.1.1",
    "file-loader": "^0.11.2",
    "http-server": "^0.10.0",
    "lodash.pick": "^4.4.0",
    "minimatch": "^3.0.4",
    "pretty": "^2.0.0",
    "pretty-data": "^0.40.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-element-to-jsx-string": "^10.1.0",
    "run-sequence": "^2.1.0"
  }
}
