{
  "name": "fuzzy-chainsaw",
  "version": "3.0.0-beta",
  "description": "Boilerplate for component-based UI development",
  "repository": "connectivedx/fuzzy-chainsaw",
  "bugs": {
    "url": "https://github.com/connectivedx/fuzzy-chainsaw/issues"
  },
  "author": "Connective DX",
  "license": "MIT",
  "directories": {
    "source": "./source",
    "dest": "./dist"
  },
  "scripts": {
    "postinstall": "npm run dll",
    "start": "http-server ./dist -p 8081",
    "dev": "gulp clean:pre && npm-run-all --parallel dev:webpack test:dev",
    "dev:webpack": "webpack-dev-server --config ./build/webpack.dev-config.js --progress",
    "dll": "gulp webpack-dll:build",
    "dll:ci": "gulp webpack-dll:build:ci",
    "dll:production": "gulp webpack-dll:production",
    "dll:production:ci": "gulp webpack-dll:production:ci",
    "build": "gulp build",
    "build:ci": "gulp build:ci",
    "production": "gulp production",
    "production:ci": "gulp production:ci",
    "full:build": "gulp full:build",
    "full:build:ci": "gulp full:build:ci",
    "full:production": "gulp full:production",
    "full:production:ci": "gulp full:production:ci",
    "test": "gulp test",
    "test:dev": "gulp test:dev",
    "watch": "gulp watch",
    "new:tag": "gulp scaffold:tag --name",
    "new:component": "gulp scaffold:component --name",
    "new:composition": "gulp scaffold:composition --name",
    "new:stateful": "gulp scaffold:component:stateful --name"
  },
  "engines": {
    "node": "^=6.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "gulp": "^3.9.1",
    "http-server": "^0.10.0",
    "karma": "^1.7.0",
    "lodash.pick": "^4.4.0",
    "npm-run-all": "^4.0.2",
    "postcss": "^6.0.8",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "reset-css": "^2.2.0",
    "stylelint-config-standard": "^16.0.0",
    "tape": "^4.7.0",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "dom-select": "^1.1.1",
    "enzyme": "^2.9.1",
    "prop-types": "^15.5.10",
    "querystring": "^0.2.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
