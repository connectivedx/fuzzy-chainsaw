{
	"extends": "airbnb",
	"rules": {
		"max-len": 0,
		"comma-dangle": ["error", "never"],
		"camelcase": 0,
		"no-param-reassign": 0,
		"no-plusplus": 0,
		"linebreak-style": 0,
		"import/no-named-as-default": 0,
		"import/no-extraneous-dependencies": 0,
		"import/no-unresolved": 0,
		"import/extensions": 0,
		"react/no-danger": 0,
		"react/jsx-pascal-case": 0,
		"react/forbid-prop-types": 0,
		"react/require-default-props": 0
	},
	"globals": {
		"document": true,
		"window": true,
		"location": true
	}
}
